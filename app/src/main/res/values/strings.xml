    <string name="test_case_speed_pitch_failure" tools:ignore="MissingTranslation">Speed/Pitch Setting Failure Test</string>
    <string name="test_case_speed_pitch_failure_desc" tools:ignore="MissingTranslation">Test: Simulate speed/pitch setting failure scenarios to verify warning display functionality. Tests include invalid parameter values and TTS engine error responses. Logs are recorded in Logcat with tag "TTS_TEST" for debugging.</string>
    
    <!-- ÊµãËØïÊ®°Âºè - ÂèëÈü≥‰∫∫‰ø°ÊÅØ -->
    <string name="test_case_log_tts_voices" tools:ignore="MissingTranslation">Log TTS Engine Voices</string>
    <string name="test_case_log_tts_voices_desc" tools:ignore="MissingTranslation">When enabled, every time you click "Speak", all supported languages and voices of the TTS engine will be printed to logcat, with default language, default voice, and the current TTS instance\'s voice marked.</string>
    
    <!-- ÊµãËØïÊ®°Âºè‰ø°ÊÅØ -->
    <string name="test_mode_info_title" tools:ignore="MissingTranslation">Test Mode Instructions</string>
    <string name="test_mode_info_content" tools:ignore="MissingTranslation"><![CDATA[
        Test Mode allows developers to test specific functionality of the application.<br/><br/>
        <b>Usage Process:</b><br/>
        ‚Ä¢ Select one or more test items by checking the checkboxes<br/>
        ‚Ä¢ The test button shows the number of selected items<br/>
        ‚Ä¢ Click "Test" to start the selected tests<br/>
        ‚Ä¢ The interface will change to indicate test mode is active<br/><br/>
        <b>Test Features:</b><br/>
        ‚Ä¢ Tests simulate specific scenarios to verify functionality<br/>
        ‚Ä¢ Test logs are recorded in Logcat with tag "TTS_TEST"<br/>
        ‚Ä¢ You can exit test mode anytime from the menu<br/>
        ‚Ä¢ Test mode does not affect normal app functionality
    ]]></string>
    
    <!-- ËØ≠ÈÄüÂíåÈü≥Ë∞É‰ø°ÊÅØ -->
    <string name="speed_info_title" tools:ignore="MissingTranslation">Speed Instructions</string>
    <string name="speed_info_content" tools:ignore="MissingTranslation"><![CDATA[
        Speed controls how fast the text is read.<br/><br/>
        <b>Range:</b> 0.5x - 2.0x<br/>
        <b>Default:</b> 1.0x (normal speed)<br/><br/>
        <b>Usage Tips:</b><br/>
        ‚Ä¢ 0.5x-0.8x: Slow reading, suitable for learning<br/>
        ‚Ä¢ 1.0x: Normal speed, daily use<br/>
        ‚Ä¢ 1.2x-2.0x: Fast reading, time-saving
    ]]></string>
    
    <string name="pitch_info_title" tools:ignore="MissingTranslation">Pitch Instructions</string>
    <string name="pitch_info_content" tools:ignore="MissingTranslation"><![CDATA[
        Pitch controls the highness or lowness of the voice.<br/><br/>
        <b>Range:</b> 0.5x - 2.0x<br/>
        <b>Default:</b> 1.0x (normal pitch)<br/><br/>
        <b>Usage Tips:</b><br/>
        ‚Ä¢ 0.5x-0.8x: Low pitch, deep voice<br/>
        ‚Ä¢ 1.0x: Normal pitch, natural voice<br/>
        ‚Ä¢ 1.2x-2.0x: High pitch, sharp voice
    ]]></string>
    
    <!-- ÂèëÈü≥‰∫∫‰ø°ÊÅØ -->
    <string name="voice_info_title" tools:ignore="MissingTranslation">Voice Instructions</string>
    <string name="voice_info_content" tools:ignore="MissingTranslation"><![CDATA[
        Voice selection allows you to choose different speakers for text-to-speech.<br/><br/>
        <b>Selection Process:</b><br/>
        ‚Ä¢ First select a language, then choose a voice<br/>
        ‚Ä¢ Each language has one or more available voices<br/>
        ‚Ä¢ Only one voice per language is marked as "(Default)"<br/><br/>
        <b>Voice Features:</b><br/>
        ‚Ä¢ Voice features are displayed below the name in [brackets], but not all voices have features<br/>
        ‚Ä¢ Features show voice capabilities and characteristics<br/>
        ‚Ä¢ Deep background indicates the currently selected voice
    ]]></string>

